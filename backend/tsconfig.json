{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["esnext", "dom"],
		"types": ["@cloudflare/workers-types", "jest"],
		"moduleResolution": "node",
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		},
		"resolveJsonModule": true
	},
	"include": ["src/**/*", "bindings.d.ts"],
}
